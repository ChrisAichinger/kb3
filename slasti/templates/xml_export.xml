<?xml version="1.0" encoding="UTF-8"?>
<posts user="{{s_username}}" tag="">
{% for mark in marks %}
    <post href="{{mark.url|e}}"
          description="{{(mark.title or mark.url)|e}}"
          tag="{{' '.join(mark.tags)|e}}"
          time="{{mark.time | format_date}}"
          extended="{{(mark.note or '')|e}}" />
{% endfor %}
</posts>
